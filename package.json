{
  "name": "an-eng-pro",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm run build:css -- --watch\" \"ng serve\"",
    "build:css": "tailwindcss -i src/styles.css -o public/styles.css",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@9.6.6",
  "dependencies": {
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.1",
    "@angular/cli": "^21.0.1",
    "@angular/compiler-cli": "^21.0.0",
    "@tailwindcss/cli": "^4.1.17",
    "autoprefixer": "^10.4.22",
    "concurrently": "^9.2.1",
    "jsdom": "^27.1.0",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}